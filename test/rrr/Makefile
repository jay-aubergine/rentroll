all:
	@echo "*** Completed in rrr ***"

clean:
	rm -f rentroll.log log llog *.g ./gold/*.g err.txt [a-z] [a-z][a-z1-9] qq? fail request
	@echo "*** CLEAN completed in rrr ***"

test:
	touch fail
	./functest.sh
	@echo "*** TEST completed in rrr ***"
	@rm -f fail

package:
	@echo "*** PACKAGE completed in rrr ***"
